<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]-Creative_Harmony</title>

        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
        <link href="~/login/vendor/animate/animate.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>

    <div class="preloader">
        <div class="status-mes">
            <div class="bigSqr">
                <div class="square first"></div>
                <div class="square second"></div>
                <div class="square third"></div>
                <div class="square fourth"></div>
            </div>
            <div class="text_loading text-center"><%=getResource("Loading") %></div>
        </div>
    </div>

    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand">
                    <img src="~/images/Asset 3mdpi.png" class="logo" />
                </a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index" id="Home">Home</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="OurService" id="OurService">Our Service</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="OurWorks" id="OurWorks">Our Works</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="OurClients" id="Clients">Our Clients</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="OurTeam" id="OurTeam">Our Team</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Contact" id="Contactus">Contact us</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="clearfix" style="margin-bottom: 6em;"></div>
    <div class="clearfix"></div>
    <partial name="_CookieConsentPartial" />
    <div class="body-content">
        @RenderBody()
        <br />
    </div>

    <div class="bg-ch padd-20">
        <div class="container">
            <div class="row divStarted">
                <div class="col-md-8">
                    <h3 class="white"> Do you have a project you would like to discuss? </h3>
                </div>
                <div class="col-md-4">
                    <button class="btn btn-started">Lets Get Started </button>
                </div>
            </div>
        </div>
    </div>
    <div class="bg-gray padd-20">
        <div class="container">
            <div class="row ">
                <div class="col-md-3">
                    <img src="~/images/footer/dpi/Asset 116ldpi.png" />
                    <span class="color-ch"> Riyadh Al - Falah neighborhood Othman bin Affan Street</span>
                </div>
                <div class="col-md-3 mt-5">
                    <img src="~/images/footer/dpi/Asset 114ldpi.png" class="callicon" />
                    <span class="color-ch">+966 58882 5547</span>
                </div>
                <div class="col-md-3 mt-5">
                    <img src="~/images/footer/dpi/Asset 115ldpi.png" />
                    <span class="color-ch"> info@ch-harmony.com</span>
                </div>
                <div class="col-md-3 mt-5 txtright">
                    <img src="~/images/footer/dpi/Asset 120ldpi.png" class="social-icon" style="width:15px;" />
                    <img src="~/images/footer/dpi/Asset 119ldpi.png" class="social-icon" />
                    <img src="~/images/footer/dpi/Asset 118ldpi.png" class="social-icon" />
                    <img src="~/images/footer/dpi/Asset 121ldpi.png" class="social-icon" />
                    <img src="~/images/footer/dpi/Asset 117ldpi.png" class="social-icon" />
                </div>
            </div>
        </div>
    </div>
    <footer class="bg-ch">
        <div class="container">
            <div class="row">
                <p class="white text-center copyright">2018 Creative Harmony. All rights resrved</p>
            </div>
        </div>
    </footer>


    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

