@{
    ViewData["Title"] = "Contact";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div id="Map" style="width:100%;height:600px;"></div>
        </div>
    </div>
</div>

@section Scripts
{
    <script>
        function myMap() {
            var mapProp = {
                center: new google.maps.LatLng(24.7016038, 46.6754026),
                zoom: 5,
            };
            var map = new google.maps.Map(document.getElementById("Map"), mapProp);
        
        var marker = new google.maps.Marker({
            position: new google.maps.LatLng(24.7253981, 46.2620413),
            animation: google.maps.Animation.BOUNCE
        });

        marker.setMap(map);

        var marker = new google.maps.Marker({
            position: new google.maps.LatLng(24.7253981, 46.2620413),
            icon: 'pinkball.png'
        });

            marker.setMap(map);
        }
    </script>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCW91WILCzESk0p7hHpbgdb9CoxkkJAI1c&callback=myMap"></script>
}

